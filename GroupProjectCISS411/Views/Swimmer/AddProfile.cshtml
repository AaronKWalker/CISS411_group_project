@model GroupProjectCISS411.Models.Swimmer
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
<h2 class="pageHeader">Add Your Profile</h2>
<form asp-action="AddProfile" class="form horizontal">
    <fieldset class="form-group">
        <input asp-for="@Model.UserId" type="hidden" />
        <label asp-for="SwimmerName" class="control-label col-xs-2"></label>
        <div class="col-xs-6">
            <input asp-for="SwimmerName" class="form-control" />
        </div>
    </fieldset>
    <div class="form-group">
        <div class="col-xs-offset-2 col-xs-10">
            <button class="btn btn-primary" type="submit">Add Profile</button>
            <p><a asp-action="Index">Back to swimmer home</a></p>
        </div>
    </div>
</form>
